<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="switch_view">
        <attr name="main_text" format="string"/>
        <attr name="sub_text" format="string"/>
        <attr name="line_visible" format="enum">
            <enum name="VISIBLE" value="0"/>
            <enum name="GONE" value="4"/>
            <enum name="INVISIBLE" value="8"/>
        </attr>
        <attr name="check" format="boolean"/>
    </declare-styleable>
</resources>